{
  "name": "promise-loading-handler",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "repository": "https://github.com/kouts/promise-loading-handler",
  "keywords": [
    "loader",
    "loading",
    "progress",
    "spinner",
    "promise",
    "handler"
  ],  
  "main": "./dist/promise-loading-handler.umd.js",
  "module": "./dist/promise-loading-handler.es.js",
  "exports": {
    ".": {
      "import": "./dist/promise-loading-handler.es.js",
      "require": "./dist/promise-loading-handler.umd.js"
    }
  },
  "unpkg": "dist/promise-loading-handler.umd.js",
  "sideEffects": false,  
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "lint-fix": "eslint --fix \"**/*.{vue,ts,js}\"",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^17.0.5",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/parser": "^5.8.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "axios": "^0.24.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.4",
    "nprogress": "^0.2.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue": "^3.2.25",
    "vue-tsc": "^0.29.8"
  }
}
